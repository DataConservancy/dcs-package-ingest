# This is the *public facing* API-X port and baseURI.  If changing the default, you need to edit the next two lines
APIX_PORT=80
APIX_BASEURI=http://localhost/fcrepo/rest
APIX_LOADER_PORT=8081

PACKAGE_INGEST_PORT=32081

# If changing the fedora host, port, or context path, you need to update all
# the FCREPO_* variables with the updated value!
FCREPO_HOST=fcrepo
FCREPO_PORT=8080
FCREPO_CONTEXT_PATH=/fcrepo
FCREPO_BASEURI=http://fcrepo:8080/fcrepo/rest
FCREPO_PROXYURI=http://fcrepo:8080/fcrepo

